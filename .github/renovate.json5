{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": ["github-releases"],
      "matchPackageNames": ["tailscale/tailscale"],
      "fileMatch": ["Containerfile"],
      "extractVersion": "tarball\/(?<version>v\\d.)+",
      "enabled": true
    }
  ]
}