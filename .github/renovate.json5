{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "dockerfile": {
    "fileMatch": ["Containerfile"],
    "packageRules": [
      {
        "matchDatasources": ["github-releases"],
        "matchPackageNames": ["tailscale/tailscale"],
        "extractVersion": "v(\\d+\\.\\d+\\.\\d+)",
        "enabled": true
      }
    ]
  }
}
